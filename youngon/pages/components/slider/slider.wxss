.youngon {
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  height: 100%;
  width: 100%;
  padding: 30rpx;
  background-image: url(http://wy.tianchenyong.top/backgroundSlider-youngon.jpg);
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.youngon-person {
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  font-size: 32rpx;
  border-bottom: 1rpx solid #EAEAEA;
}
.youngon-input {
  height: 60rpx;
}
.youngon-soso {
  min-height: 100rpx;
  display: flex;
  justify-content: space-between
}
.youngon-soso_left {
  width: 70%;
  min-height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-bottom: 1px solid #3aafec;
}
.youngon-soso_right {
  width: 20%;
  display: flex;
  min-height: 100rpx;
  justify-content: center;
  align-items: center;
}
.placeholder {
  font-size: 24rpx;
}
.youngon-soso_logo {
  width: 100%;
  height: 80%;
  border-radius: 50%;
  background: #3aafec;
  display: flex;
  justify-content: center;
  align-items: center;
  box-shadow: 1px 1px 3px rgba(0, 0, 0, 0.3)
}
.youngon-persons {
  display: flex;
  box-sizing: border-box;
  padding: 10rpx;
  flex: 1
}
.youngon-persons_list {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 150rpx;
}
.youngon-persons_list view:nth-child(1) {
  width: 40%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.youngon-list_headerImg {
  height: 100rpx;
  width: 100rpx;
  border-radius: 50%;
  border: 2px solid #3aafec;
  overflow: hidden;
  background-image: url(data:image/png;base64,)
}
.youngon-list_name {
  display: flex;
  flex: 1;
  font-size: 36rpx;
  justify-content: center;
  align-items: center;
}
.youngon-more {
  display: flex;
  box-sizing: border-box;
  padding: 10rpx;
  min-height: 50rpx
}
.youngon-more_logo {
  width: 30%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.youngon-more_title {
  font-size: 28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex: 1
}

.soso-size {
  font-size: 50rpx!important;
  color: white;
}
.gb-change {
  background: rgba(0, 0, 0, 0.1);
}







