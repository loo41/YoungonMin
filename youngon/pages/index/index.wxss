/* 侧边栏 */
.curtain {
    position: fixed;
    width: 100%;
    left: -100%;
    bottom: 0;
    top: 0;
    background: rgba(0, 0, 0, 0.3);
    z-index: 10;
}
.sidebar {
    position: fixed;
    left: 0;
    bottom: 0;
    left: -60%;
    top: 0;  
    width: 60%;
    z-index: 11;
    background: #FFFFFF;
    background-repeat: no-repeat;
    background-size: 100%;
}
.slider {
    height: 100%;
    width: 100%;
    margin: 0rpx;
    padding: 0rpx;
}


.youngon-active {
    box-sizing: border-box;
    min-height: 70rpx;
    padding-left: 5%;
    padding-right: 5%;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.youngon-active_title {
    font-size: 32rpx;
    padding-left: 30rpx;
    padding-right: 30rpx;
}
.youngon-active_line {
    height: 49%;
    border-bottom: 1rpx dashed #3aafec;
    flex: 1
}
.youngonTrends {
    box-sizing: border-box;
    width: 100%;
    padding: 3%;
    background: white;
}
.youngonTrends-item {
    height: 200rpx;
    min-height: 200rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 10rpx;
}
.youngonTrends-picture {
    width: 40%;
    height: 100%;
    box-sizing: border-box;
    display: flex;
    justify-content: center;
}
.youngonTrends-title {
    display: flex;
    box-sizing: border-box;
    padding: 10rpx;
    padding-left: 5%;
    height: 100%;
    font-size: 34rpx;
    flex-direction: column;
    justify-content: space-between;
    align-items: flex-start;
    flex: 1;
}
.title {
    font-size: 30rpx;
    margin-bottom: 10%;
}
.youngonTrends-bottom {
    height: 70rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 28rpx;
    letter-spacing: 4rpx;
}
.msg {
    height: 100%;
    width: 100%;
    position: fixed;
    top: 0
}
.gb-change {
    background: rgba(0, 0, 0, 0.1);
}