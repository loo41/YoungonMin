@keyframes around {
  from {
   margin-left: 100%;
  }
  to {
   margin-left: -200%;
  }
}
 .marquee_box {
    display: flex;
    align-items: center;
 }
 .marquee_text{
    display: inline-block;
    white-space: nowrap;
    font-size: 24rpx;
    animation-name: around;
    animation-duration: 10s;
    letter-spacing: 2rpx;
    animation-iteration-count: infinite;
    animation-timing-function:linear;
}
.close_marquee {
    position: absolute;
    height: 60rpx;
    width: 60rpx;
    background-color: rgba(0, 0, 0, 0.3);
    right: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    color: white;
}
.icon-gonggao {
    margin-right: 6rpx;
}
.moduleContainer {
    min-height: 300rpx;
    box-sizing: border-box;
    padding-top: 20rpx;
    padding-left: 40rpx;
    padding-right: 40rpx;
    padding-bottom: 20rpx;
}
.moduleContainer view {
    display: flex;
    min-height: 150rpx;
    justify-content: space-between;
    align-items: center;
    font-size: 32rpx;
}
.moduleContainer view view {
    width: 49%;
    display: flex;
    justify-content: center;
    margin-top: 2%;
    background: #FFFFFF;
    border-radius: 6rpx;
}
.header-Img {
    width: 40%;
    box-sizing: border-box;
    display: flex;
    justify-content: center;
    align-items: center;
}
.header-Img-box {
    height: 100rpx;
    width: 100rpx;
    border: 1rpx solid #3aafec;
    border-radius: 50%;
    background-position-x: center;
    background-position-y: center;
    background-repeat: no-repeat;
}
