.customerService {
    position: fixed;
    height: auto;
    right: 0;
    overflow: hidden;
    top: 45%;
}
.customerService view{
    width: 100rpx;
    height: 100rpx;
    box-sizing: border-box;
    margin-top: 4%;
    border-radius: 4%;
}
.share {
