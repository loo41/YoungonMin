  background: white;
  box-sizing: border-box;
  padding: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.share-happly {
  font-size: 24rpx;
}
.share-but {
  display: flex;
  flex: 1
}